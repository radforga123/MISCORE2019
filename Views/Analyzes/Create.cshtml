@model MISCORE2019.Models.Analyze

@{ ViewData["Title"] = "Create"; }

<head>
    <link rel="stylesheet" type="text/css" href="~/css/AnalyzecCreate.css" />
</head>

<div class="basic-div">
    <div class="create-div">
        <form asp-action="Create">
            <div>
                <label asp-for="weight" class="control-label"></label>
                <input asp-for="weight" class="form-control" />
                <span asp-validation-for="weight" class="text-danger"></span>
            </div>
            <div>
                <label asp-for="diagnos" class="control-label"></label>
                <input asp-for="diagnos" class="form-control" />
                <span asp-validation-for="diagnos" class="text-danger"></span>
            </div>
            <div>
                <label asp-for="PatientID" class="control-label"></label>
                <input asp-for="PatientID" class="form-control" />
                <span asp-validation-for="PatientID" class="text-danger"></span>
            </div>
            <div class="div-buttons">
                <button asp-action="Create">
                    <a>Создать</a>
                </button>
                <button asp-action="Index">
                    <a>Вернуться</a>
                </button>
            </div>
        </form>
    </div>
</div>